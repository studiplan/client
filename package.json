{
  "name": "webapp-template",
  "version": "1.0.0",
  "description": "preact + babel + webpack + eslint + typescript + workbox template",
  "main": "src/index.tsx",
  "scripts": {
    "test": "npm run lint && npm run typecheck",
    "start": "webpack-dev-server --open --config webpack.dev.config.js",
    "build": "webpack --config webpack.prod.config.js",
    "lint": "eslint --config .eslintrc.yml --ext jsx --ext js --ext ts --ext tsx src",
    "lint:fix": "npm run lint -- --fix",
    "typecheck": "tsc",
    "test:prod": "http-server dist",
    "update": "ncu",
    "gen": "graphql-codegen --config codegen.yml"
  },
  "pre-commit": [
    "lint:fix"
  ],
  "keywords": [
    "preact",
    "webpack",
    "babel",
    "eslint",
    "typescript",
    "workbox",
    "serviceworker",
    "template"
  ],
  "author": "Felix Breuer",
  "license": "MIT",
  "dependencies": {
    "@apollo/react-hoc": "^3.1.3",
    "@apollo/react-hooks": "^3.1.3",
    "@babel/runtime": "^7.7.2",
    "apollo-boost": "^0.4.7",
    "graphql": "^14.5.8",
    "preact": "^10.0.5",
    "prop-types": "^15.7.2",
    "semantic-ui-react": "^0.88.1",
    "unistore": "^3.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-react": "^7.7.0",
    "@babel/preset-typescript": "^7.7.4",
    "@graphql-codegen/cli": "^1.11.2",
    "@types/react": "^16.9.13",
    "@types/react-dom": "^16.9.4",
    "@typescript-eslint/eslint-plugin": "^2.8.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "eslint": "^6.6.0",
    "eslint-plugin-react": "^7.16.0",
    "html-webpack-plugin": "^3.2.0",
    "http-server": "^0.11.1",
    "npm-check-updates": "^3.2.1",
    "pre-commit": "^1.2.2",
    "typescript": "^3.7.2",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0",
    "webpack-merge": "^4.2.2",
    "workbox-webpack-plugin": "^4.3.1",
    "@graphql-codegen/typescript": "1.11.2",
    "@graphql-codegen/typescript-operations": "1.11.2",
    "@graphql-codegen/typescript-react-apollo": "1.11.2"
  }
}
